public interface RequestDao {

	// CRUD OPERTAIONS
	public void insertCompleteRequest(Request request);
	
	//create
	// eventlocation, event, grade, request
	public int insertEventLocation(Request request);
	public boolean insertEvent(Request request);
	public boolean insertGrade(Request request);
	public boolean insertRequest(Request request);

	
	//select (read)
	public int getLocationId(String address, String city, String state, String zip, String country);
	public List<Request> getRequests(int empId);
}
